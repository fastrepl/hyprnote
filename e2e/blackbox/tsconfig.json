{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "types": ["@wdio/globals/types", "@wdio/mocha-framework"]
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
