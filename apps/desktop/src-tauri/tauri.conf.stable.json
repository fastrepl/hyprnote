{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Hyprnote",
  "mainBinaryName": "Hyprnote",
  "identifier": "com.hyprnote.stable",
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://desktop.hyprnote.com/update/{{target}}-{{arch}}/{{current_version}}?channel=stable"
      ]
    }
  },
  "bundle": {
    "macOS": {
      "files": {
        "embedded.provisionprofile": "assets/hyprnote-stable.provisionprofile"
      }
    }
  }
}
