{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "positioner:default",
    "fs:default",
    "store:default",
    "shell:allow-open",
    {
      "identifier": "http:default",
      "allow": [{ "url": "https://api.openai.com" }]
    }
  ]
}
