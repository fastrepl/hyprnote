root ::= thinking sectionf section section section? section?

sectionf ::= "# Objective\n\n" line line? line? "\n"
section ::= header "\n\n" bline bline bline? bline? bline? "\n"
header ::= "# " [^*.\n]+

line ::= "- " [A-Z] [^*.\n[(]+ ".\n"
bline ::= "- **" [^*\n:]+ "**: " ([^*;,[.\n] | link)+ ".\n"

hsf ::= "- Objective\n"
hd ::= "- " [A-Z] [^[(*\n]+ "\n"
thinking ::= "<headers>\n" hsf hd hd? hd? hd? "</headers>"

link ::= "[" [^\]]+ "]" "(" [^)]+ ")"