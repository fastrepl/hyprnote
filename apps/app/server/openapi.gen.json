{
  "openapi": "3.1.0",
  "info": {
    "title": "",
    "version": ""
  },
  "paths": {
    "/health": {
      "get": {}
    },
    "/api/native/user/integrations": {
      "get": {
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/NangoIntegration"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/native/subscription": {
      "get": {
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Subscription"
                }
              }
            }
          }
        }
      }
    },
    "/api/native/create_title": {
      "post": {
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateTitleRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CreateTitleResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/native/live_summary": {
      "post": {
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LiveSummaryRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LiveSummaryResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/native/postprocess_enhance": {
      "post": {
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PostprocessEnhanceRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PostprocessEnhanceResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/web/connect": {
      "post": {
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ConnectInput"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ConnectOutput"
                }
              }
            }
          }
        }
      }
    },
    "/api/web/session/{id}": {
      "get": {
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Session"
                }
              }
            }
          }
        }
      }
    },
    "/api/web/integration/connection": {
      "post": {
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/NangoConnectSessionRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NangoConnectSessionResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearer_token": {
        "type": "http",
        "scheme": "Bearer"
      }
    },
    "schemas": {
      "Config": {
        "type": "object",
        "required": [
          "general",
          "id",
          "notification",
          "user_id"
        ],
        "properties": {
          "general": {
            "$ref": "#/components/schemas/ConfigGeneral"
          },
          "id": {
            "type": "string"
          },
          "notification": {
            "$ref": "#/components/schemas/ConfigNotification"
          },
          "user_id": {
            "type": "string"
          }
        }
      },
      "ConfigGeneral": {
        "type": "object",
        "required": [
          "autostart",
          "display_language",
          "jargons",
          "speech_language",
          "tags"
        ],
        "properties": {
          "autostart": {
            "type": "boolean"
          },
          "display_language": {
            "type": "string",
            "pattern": "^[a-zA-Z]{2}$"
          },
          "jargons": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "speech_language": {
            "type": "string",
            "pattern": "^[a-zA-Z]{2}$"
          },
          "tags": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "ConfigNotification": {
        "type": "object",
        "required": [
          "auto",
          "before"
        ],
        "properties": {
          "auto": {
            "type": "boolean"
          },
          "before": {
            "type": "boolean"
          }
        }
      },
      "ConnectInput": {
        "type": "object",
        "required": [
          "code",
          "fingerprint",
          "user_id"
        ],
        "properties": {
          "code": {
            "type": "string"
          },
          "fingerprint": {
            "type": "string"
          },
          "org_id": {
            "type": [
              "string",
              "null"
            ]
          },
          "user_id": {
            "type": "string"
          }
        }
      },
      "ConnectOutput": {
        "type": "object",
        "required": [
          "human_id",
          "key"
        ],
        "properties": {
          "human_id": {
            "type": "string"
          },
          "key": {
            "type": "string"
          }
        }
      },
      "ConversationChunk": {
        "type": "object",
        "required": [
          "diarizations",
          "end",
          "local_audio_path",
          "remote_audio_path",
          "start",
          "transcripts"
        ],
        "properties": {
          "diarizations": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/DiarizationChunk"
            }
          },
          "end": {
            "type": "string",
            "format": "date-time"
          },
          "local_audio_path": {
            "type": "string"
          },
          "remote_audio_path": {
            "type": "string"
          },
          "start": {
            "type": "string",
            "format": "date-time"
          },
          "transcripts": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TranscriptChunk"
            }
          }
        }
      },
      "CreateTitleRequest": {
        "type": "object",
        "required": [
          "config",
          "timeline_view"
        ],
        "properties": {
          "config": {
            "$ref": "#/components/schemas/Config"
          },
          "timeline_view": {
            "$ref": "#/components/schemas/TimelineView"
          }
        }
      },
      "CreateTitleResponse": {
        "type": "object",
        "required": [
          "title"
        ],
        "properties": {
          "title": {
            "type": "string"
          }
        }
      },
      "DiarizationChunk": {
        "type": "object",
        "required": [
          "end",
          "speaker",
          "start"
        ],
        "properties": {
          "end": {
            "type": "integer",
            "format": "uint64",
            "minimum": 0.0
          },
          "speaker": {
            "type": "string"
          },
          "start": {
            "type": "integer",
            "format": "uint64",
            "minimum": 0.0
          }
        }
      },
      "LiveSummaryRequest": {
        "type": "object",
        "required": [
          "config",
          "timeline_view"
        ],
        "properties": {
          "config": {
            "$ref": "#/components/schemas/Config"
          },
          "timeline_view": {
            "$ref": "#/components/schemas/TimelineView"
          }
        }
      },
      "LiveSummaryResponse": {
        "type": "object",
        "required": [
          "blocks"
        ],
        "properties": {
          "blocks": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/SummarizeTranscriptChunk"
            }
          }
        }
      },
      "Membership": {
        "type": "string",
        "enum": [
          "Trial",
          "Basic",
          "Pro"
        ]
      },
      "NangoConnectSessionRequest": {
        "type": "object",
        "required": [
          "allowed_integrations",
          "end_user"
        ],
        "properties": {
          "allowed_integrations": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "end_user": {
            "$ref": "#/components/schemas/NangoConnectSessionRequestUser"
          },
          "organization": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/NangoConnectSessionRequestOrganization"
              },
              {
                "type": "null"
              }
            ]
          }
        }
      },
      "NangoConnectSessionRequestOrganization": {
        "type": "object",
        "required": [
          "id"
        ],
        "properties": {
          "display_name": {
            "type": [
              "string",
              "null"
            ]
          },
          "id": {
            "type": "string"
          }
        }
      },
      "NangoConnectSessionRequestUser": {
        "type": "object",
        "required": [
          "id"
        ],
        "properties": {
          "display_name": {
            "type": [
              "string",
              "null"
            ]
          },
          "email": {
            "type": [
              "string",
              "null"
            ]
          },
          "id": {
            "type": "string"
          }
        }
      },
      "NangoConnectSessionResponse": {
        "oneOf": [
          {
            "type": "object",
            "required": [
              "data"
            ],
            "properties": {
              "data": {
                "type": "object",
                "required": [
                  "expires_at",
                  "token"
                ],
                "properties": {
                  "expires_at": {
                    "type": "string"
                  },
                  "token": {
                    "type": "string"
                  }
                }
              }
            },
            "additionalProperties": false
          },
          {
            "type": "object",
            "required": [
              "error"
            ],
            "properties": {
              "error": {
                "type": "object",
                "required": [
                  "code"
                ],
                "properties": {
                  "code": {
                    "type": "string"
                  }
                }
              }
            },
            "additionalProperties": false
          }
        ]
      },
      "NangoIntegration": {
        "type": "string",
        "enum": [
          "google-calendar",
          "outlook-calendar"
        ]
      },
      "PostprocessEnhanceRequest": {
        "type": "object",
        "required": [
          "editor"
        ],
        "properties": {
          "editor": {
            "type": "string"
          }
        }
      },
      "PostprocessEnhanceResponse": {
        "type": "object",
        "required": [
          "editor"
        ],
        "properties": {
          "editor": {
            "type": "string"
          }
        }
      },
      "Session": {
        "type": "object",
        "required": [
          "conversations",
          "id",
          "raw_memo_html",
          "timestamp",
          "title",
          "user_id"
        ],
        "properties": {
          "audio_local_path": {
            "type": [
              "string",
              "null"
            ]
          },
          "audio_remote_path": {
            "type": [
              "string",
              "null"
            ]
          },
          "calendar_event_id": {
            "type": [
              "string",
              "null"
            ]
          },
          "conversations": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ConversationChunk"
            }
          },
          "enhanced_memo_html": {
            "type": [
              "string",
              "null"
            ]
          },
          "id": {
            "type": "string"
          },
          "raw_memo_html": {
            "type": "string"
          },
          "timestamp": {
            "type": "string",
            "format": "date-time"
          },
          "title": {
            "type": "string"
          },
          "user_id": {
            "type": "string"
          }
        }
      },
      "Subscription": {
        "type": "object",
        "required": [
          "membership"
        ],
        "properties": {
          "membership": {
            "$ref": "#/components/schemas/Membership"
          }
        }
      },
      "SummarizeTranscriptChunk": {
        "type": "object",
        "required": [
          "points"
        ],
        "properties": {
          "points": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "TimelineView": {
        "type": "object",
        "required": [
          "items"
        ],
        "properties": {
          "items": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TimelineViewItem"
            }
          }
        }
      },
      "TimelineViewItem": {
        "type": "object",
        "required": [
          "end",
          "speaker",
          "start",
          "text"
        ],
        "properties": {
          "end": {
            "type": "integer",
            "format": "uint64",
            "minimum": 0.0
          },
          "speaker": {
            "type": "string"
          },
          "start": {
            "type": "integer",
            "format": "uint64",
            "minimum": 0.0
          },
          "text": {
            "type": "string"
          }
        }
      },
      "TranscriptChunk": {
        "type": "object",
        "required": [
          "end",
          "start",
          "text"
        ],
        "properties": {
          "end": {
            "type": "integer",
            "format": "uint64",
            "minimum": 0.0
          },
          "start": {
            "type": "integer",
            "format": "uint64",
            "minimum": 0.0
          },
          "text": {
            "type": "string"
          }
        }
      }
    }
  }
}