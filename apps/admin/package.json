{
  "name": "@hypr/admin",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "redo": "rm -rf src/lib/db/migrations && pnpm -F admin auth:generate && pnpm -F admin drizzle:generate && rm db.sqlite && pnpm -F admin drizzle:migrate",
    "typecheck": "tsc --noEmit",
    "auth:generate": "npx @better-auth/cli@latest generate --output src/lib/db/schema/auth.ts --config src/lib/auth/index.ts -y",
    "drizzle:generate": "npx drizzle-kit generate",
    "drizzle:push": "npx drizzle-kit push",
    "drizzle:migrate": "npx drizzle-kit migrate",
    "dev": "vite dev",
    "build": "vite build && tsc --noEmit",
    "start": "node .output/server/index.mjs"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.19",
    "@cerbos/grpc": "^0.22.1",
    "@libsql/client": "^0.15.12",
    "@mantine/core": "^8.2.5",
    "@mantine/form": "^8.2.5",
    "@mantine/hooks": "^8.2.5",
    "@mantine/notifications": "^8.2.5",
    "@tabler/icons-react": "^3.34.1",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-router": "^1.131.27",
    "@tanstack/react-router-with-query": "^1.130.17",
    "@tanstack/react-start": "^1.131.27",
    "ai": "^5.0.21",
    "better-auth": "^1.3.7",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "drizzle-zod": "^0.8.3",
    "lucide-react": "^0.525.0",
    "mantine-form-zod-resolver": "^1.3.0",
    "openid-client": "^6.6.4",
    "posthog-js": "^1.260.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-preset-mantine": "^2.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^22.17.2",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.1",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
